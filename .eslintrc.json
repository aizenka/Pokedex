{
"env": {
    "browser": true,
    "es6": true,
    "node": true
},
"globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
},
"parser": "babel-eslint",
"parserOptions": {
    "ecmaFeatures": {
    "legacyDecorators": true,
    "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
},
"plugins": [
    "react"
],
"rules": {
    "prefer-const": "off",
    "react/jsx-handler-names": "off",
    "react/prop-types": "off"
}
}